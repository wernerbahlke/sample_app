<h1><%= @title %></h1>

<div class="search_form">
  <%= form_tag users_path, :method => 'get' do %>
    <p>
      Find user:
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
  <% end %>
</div>

<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
